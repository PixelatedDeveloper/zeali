{{!< default}}
{{! The tag above means - insert everything in this file into the {body} of the default.hbs template }}
  <div class="row h-100 page-row">
  {{#post}}
      <div class="col col-xs-12 col-md-12 col-lg-12 main-content">
        <article>
          <div class="post-head">
                <h1 class="text-center post-title">{{title}}</h1>
                  <p>{{@site.title}}:~$ cd posts\{{slug}}\</p><br />
                  <p>{{@site.title}}:~$ ls -l</p><br />
                  <p>-rwx------ {{#primary_author}}{{name}} {{name}}{{/primary_author}} <time datetime="{{date format="YYYY-MM-DD"}}">{{date format="DD MMMM YYYY"}}</time> {{title}}.ext</p><br />
                  <p>{{@site.title}}:~$ ./{{title}}.ext</p>
          </div>

         <div class="d-block d-sm-block d-md-block d-lg-block tag_head_wrap">
          {{#if feature_image}}
            <img class="tag_heading text-center" src="{{img_url feature_image}}" title="{{title}}" alt="{{title}}" />
          {{/if}}
          </div>

          <div class="post-content max-content-width">
            {{content}}

              <div class="post-tags-wrapper">
              {{#if tags}}
                <div class="tagslist">
                  {{#foreach tags}}
                      <a href="{{@site.url}}/tag/{{slug}}">#{{name}}</a>
                    {{/foreach}}
              </div>
              {{/if}}<!-- end tags if -->
              </div><!-- end cat links -->
          </div>
            {{> 'post/author-section'}}

            {{> 'post/related-posts'}}
        </article>
    </div>
{{/post}}
</div>
